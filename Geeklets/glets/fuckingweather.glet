	<string>#!/usr/bin/php
&lt;?php
$ch = curl_init("http://thefuckingweather.com/?where=90029");
curl_setopt($ch, CURLOPT_HEADER, 0);
curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);
curl_setopt($ch, CURLOPT_USERAGENT, 'Mozilla/5.0 (Windows; U; Windows NT 6.1; en-US; rv:1.9.2.12) Gecko/20101026 Firefox/3.6.12');
$html = curl_exec($ch);
curl_close($ch);

$location = get_string_between($html,'&lt;span id="locationDisplaySpan" class="small"&gt;','&lt;/span&gt;');

$html = get_string_between($html,'&lt;div class="content"&gt;','&lt;table id="inputArea"'); //cut down size of variable

$temp = get_string_between($html,'&lt;p class="large"&gt;','&lt;/p&gt;');
$temp = str_replace("&amp;#176;","Â°",$temp);
$temp = trim(strip_tags($temp));

$remark = get_string_between($html,'&lt;div class="remarkContainer"&gt;','&lt;/div&gt;');
$remark = trim(strip_tags($remark));

$flavor = get_string_between($html,'&lt;p class="flavor"&gt;','&lt;/p&gt;');
$flavor = trim(strip_tags($flavor));

echo $location."\n".$temp."\n".$remark."\n".$flavor;

function get_string_between($string, $start, $end){
	$string = " ".$string;
	$ini = strpos($string,$start);
	if ($ini == 0) return "";
	$ini += strlen($start);
	$len = strpos($string,$end,$ini) - $ini;
	return substr($string,$ini,$len);
}
